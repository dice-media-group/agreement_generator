


<div class="row">
  <div class="col-sm-6">
    <h1>Payment_schedules</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_payment_schedule_path, class: 'btn btn-primary' do %>
    Add New Payment schedule
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>Document</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @payment_schedules.each do |payment_schedule| %>
        <%= content_tag :tr, id: dom_id(payment_schedule), class: dom_class(payment_schedule) do %>
          

                      <td><%= payment_schedule.document %></td>
          
          
            <td><%= link_to 'Show', payment_schedule %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
